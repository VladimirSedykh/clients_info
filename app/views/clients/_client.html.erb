<tr class="table-block">
  <td class="clickable-cell" style="font-size: 15px;"> <%= link_to client.name, "#", :attr => client.id, :class => "iframe-link" %></td>
  <% if session[:group] == "search" %>
    <td> <%= Client::GROUPS[client.role] %></td>
  <% end %>
  <td> <%= client.short_contacts %> </td>
  <td> <%= client.state %> </td>
  <td> <%= client.address %> </td>
  <td> <%= client.activity %> </td>
  <td> <%= short_date client.created_at %></td>
</tr>
